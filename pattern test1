#1...                                             1
n=int(input())                                    1 2
for i in range(1,n+1):                            1 2 3
    for j in range(1,i+1):                        1 2 3 4
        print(j,end=" ")
    print()    
    
    
  #2....                                          1
    n=int(input())                                2 2
    for i in range(n+1):                          3 3 3
        for j in range(i):                        4 4 4 4
            print(i,end=" ")                      5 5 5 5 5
        print()    
        
        
  #3....                                         5 4 3 2 1
  n=int(input())                                 5 4 3 2
  for i in range(1,n+1):                         5 4 3
      for j in range(n,i-1,-1):                  5 4 
          print(j,end=" ")                       5
      print()    
     
     
     
 #4....                                          1 
 n=int(input())                                  2 7
 for i in range(n):                              3 8 13
    val=i+1                                      4 9 14 19
    for j in range(i+1):                         5 10 15 20 25
        print(val,end=" ")
        val=val+5
    print() 
    
   
   #5....                                       1
   n=int(input())                               2 3
   c=1                                          4 5 6 
   s=1                                          7 8 9 10
   for i in range(0,n):
      for j in range(0,s):
          print(c,end=" ")
          c+=1
      print()
      s+=1
